    <script>
        // change header background after scrolling

        $(document).ready(function () {
            var scroll_start = 0;
            var startchange = $('#startchange');
            var offset = startchange.offset();
            $(document).scroll(function () {
                scroll_start = $(this).scrollTop();
                if (scroll_start > 550) {
                    $('.site-header').css('background-color', '#556575').fadeTo('slow', 50);
                } else {
                    $('.site-header').css('background-color', 'transparent');
                }
            });
        });

    </script>

    <script>
        $(function () {
            $("#contactForm").submit(function () {
                $.post("/Home/SendEmail", $(this).serialize(), function (data) {
                    if (data.success) {
                        $("#txtAdSoyad").val("");
                        $("#txtEmail").val("");
                        $("#txtMesaj").val("");
                    }
                    alert(data.message);
                });
                return false;
            })
        });
    </script>

